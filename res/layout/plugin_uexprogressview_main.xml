<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/plugin_uexprogress_bg"
    android:background="@android:color/transparent"
    android:gravity="center"
    android:orientation="vertical" >

    <org.zywx.wbpalmstar.plugin.uexprogressview.NumberProgressBar
        android:id="@+id/numberbar"
        style="@style/NumberProgressBar_Default"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/plugin_uexprogress_padding_horizontal"
        android:paddingRight="@dimen/plugin_uexprogress_padding_horizontal"
        android:visibility="gone"
        custom:progress="0" />

    <org.zywx.wbpalmstar.plugin.uexprogressview.RoundCornerProgressBar
        android:id="@+id/progress_two"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:backgroundPadding="@dimen/plugin_uexprogress_corner_padding"
        app:backgroundRadius="@dimen/plugin_uexprogress_corner_radius"
        app:max="100"
        app:progress="50" />

    <org.zywx.wbpalmstar.plugin.uexprogressview.RoundProgressBar
        android:id="@+id/roundProgressBarFill"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

</LinearLayout>