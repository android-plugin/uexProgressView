<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical" >

    <org.zywx.wbpalmstar.plugin.uexprogressview.NumberProgressBar
        android:id="@+id/numberbar"
        style="@style/NumberProgressBar_Default"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:visibility="gone"
        custom:progress="0" />

    <org.zywx.wbpalmstar.plugin.uexprogressview.RoundCornerProgressBar
        android:id="@+id/progress_two"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:visibility="gone"
        app:backgroundPadding="2dp"
        app:backgroundRadius="2dp"
        app:max="100"
        app:progress="50" />

    <org.zywx.wbpalmstar.plugin.uexprogressview.RoundProgressBar
        android:id="@+id/roundProgressBarFill"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

</LinearLayout>